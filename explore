local bottp = NpcTab:AddButton({
	Name = "Warp To Bot",
	Callback = function()
      	if selectedbot and not selectedbot:FindFirstChild("InBattle") and not TPDebounce then
			TPDebounce = true
			CanTP = false
			local botpos = selectedbot:GetPivot()
			local oldpos = char:GetPivot()
			char:PivotTo(botpos)
			task.wait()
			char:PivotTo(oldpos)
			task.wait(3)
			TPDebounce = false
		end
  	end    
})

